            <field name="name">software.story.point.base.list</field>
            <field name="model">software.story.point.base</field>
            <field name="arch" type="xml">
                <list editable="bottom">
                    <field name="profession"/>
                    <field name="soft_stage"/>
                    <field name="team"/>
